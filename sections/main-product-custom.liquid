           <script>
            document.addEventListener("DOMContentLoaded", () => {
              const element = document.querySelector('#shopify-section-{{section.id}}');
              const sticky_button = document.querySelector('.sticky_button_custom_wrapper');
            
              let isAtBottom = false;
              function isElementAtBottom() {
                return element.getBoundingClientRect().bottom <= window.innerHeight;
              }
              function toggleAction() {
                if (isElementAtBottom() !== isAtBottom) {
                   if (isElementAtBottom())
                   sticky_button.style.bottom = "0px"
                   else
                   sticky_button.style.bottom = "-130px"
                
                  isAtBottom = !isAtBottom;
                }
              }    
              window.addEventListener('scroll', toggleAction);
            });            
            </script>
