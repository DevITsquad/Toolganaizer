    <div class="swiper-wrapper">
      {% for link in product.images %}
        <div class="swiper-slide">
          <img  class="swiper-small-img" src="{{link | image_url}}" alt="product_image" loading='lazy'/>
        </div>
      {% endfor %}
    </div>
 
