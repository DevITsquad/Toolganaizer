<section>
 {{ 'component-price.css' | asset_url | stylesheet_tag }} 
  {%- style -%}

  /* Main Product Custom Wrapper */
  .main-product-custom_wrapper {
    max-width: 1270px;
    margin: auto;
    margin-top: 70px;
    justify-content: space-between;
    padding: 0 12px;
  }

  .sticky_button_custom_wrapper{
    width: 100%;
    position: fixed;
    bottom: -130px;
    left: 0;
    display: flex;
    justify-content: space-between;
    align-items: center;
    background: wheat;
    z-index: 50;
    padding: 7px 20px;
    transition:.5s;
  }
  .sticky_button_custom_wrapper .custom-price{
    margin:0;
  }
  .sticky_button_custom_wrapper .inventory_quantity_bage{
    display:none !important;
  }
  .sticky_button_custom_tittle-price{
    display:flex;
    align-items:center;
    gap:7px;
  }
   .sticky_button_custom_tittle-price h3{
    margin:0;
    padding-right:7px;
    text-align:center;
    font-size:18px;
   }

  /* Main Product Custom Slider Wrapper */
  .main-product-custom_slider_wrapper {
    max-width: 608px !important;
    max-height: 700px;
    margin: 0;
  }
  .main-product-custom_slider_wrapper .swiper-main-img{
  cursor:zoom-in;
  }
  .main-product-custom_slider_wrapper .swiper-main-img,
  .main-product-custom_slider_wrapper .swiper-small-img {
    object-fit: cover;
  }

  .main-product-custom_slider_wrapper .mySwiper_product,
  .main-product-custom_slider_wrapper .mySwiper_product .swiper-wrapper {
    max-height: 96px;
  }

  .main-product-custom_slider_wrapper .mySwiper_product {
    max-width: 473px;
    margin: auto;
    box-sizing: border-box;
    padding: 0 5px;
    margin-top: 15px;
  }

  .main-product-custom_slider_wrapper .swiper-slide .swiper-main-img,
  .main-product-custom_slider_wrapper .swiper-slide .swiper-main-img {
    max-width: 100%;
    height: 100%;
  }

  /* Main Product Custom Slider 2 */
  .main-product-custom_slider_wrapper .mySwiper_product2 {
    height: 582px;
  }

  .main-product-custom_slider_wrapper .mySwiper_product2 .swiper-main-img_wrapper {
    display: flex;
    justify-content: center;
    align-items: center;
  }

  /* .mySwiper_product2 .swiper-slide {
    box-sizing: border-box;
  } */

  .swiper-main-img_wrapper {
    border: 1px solid rgba(216, 212, 212, 1);
    box-sizing: border-box;
    width: 100%;
    height: 100%;
  }

  .main-product-custom_slider_wrapper .mySwiper_product2 .swiper-wrapper .swiper-slide .swiper-main-img {
    max-height: 589px;
  }

  .main-product-custom_slider_wrapper .swiper-slide .swiper-small-img {
    height:77px;
    width:77px;
    border: 1px solid rgba(216, 212, 212, 1);
    border-radius: 50%;
  }

  .main-product-custom_slider_wrapper .swiper-slide-thumb-active .swiper-small-img {
    border: 3px solid rgba(216, 212, 212, 1);
  }

  /* Product Swiper Buttons */

  .main-product-custom_bottom_slider-wrapper {
    display: flex;
    justify-content: space-between;
    align-items: center;
    min-width: 0;
  }

  .product_swiper-button-prev,
  .product_swiper-button-next {
    display: flex;
    justify-content: center;
    align-items: center;
    cursor: pointer;
  }

  .qty_and_buy-btn_wrapper {
    display: flex;
    justify-content: space-between;
    align-items: center;
    gap: 18px;
    margin-top: 24px;
  }

  .product__accordion.accordion.quick-add-hidden details summary {
    min-height: 72px;
    border-top: solid 1px rgba(217, 217, 217, 1);
    display: flex;
    align-items: center;
    justify-content:space-between;
  }
  .product__accordion.accordion.quick-add-hidden details[open] svg{
    transform: rotate(90deg);
  }
  .custom_product_under_tittle-main {
    font-family: var(--custom-font-style-primary);
    font-size: 20px;
    font-weight: 700;
    line-height: 28px;
    letter-spacing: 0em;
    color: var(--custom-font-color-primary);
    max-width: 566px;
  }
  .product__title h1 {
    font-family: var(--custom-font-style-primary);
    font-size: 50px;
    font-weight: 700;
    line-height: 55px;
    letter-spacing: 0em;
    color: var(--custom-font-color-primary);
  }

  .product_section_description,
  .product_section_description li,
  .product_section_description li p {
    margin: 0;
    padding: 0;
  }
  .product_section_description li {
    display: flex;
    gap: 15px;
    align-items: center;
    margin-bottom: 16px;
  }
  .product_section_description li svg {
    min-height: 14px;
    min-width: 18px;
    height: 14px;
    width: 18px;
  }
  .product_section_description li p {
    font-family: var(--custom-font-style-secondary);
    font-size: 20px;
    font-weight: 500;
    line-height: 23px;
    letter-spacing: 0em;
    color: var(--custom-font-color-primary);
  }

  .custom-accordion__title{
    font-family: var(--custom-font-style-primary);
    font-size: 18px;
    font-weight: 800;
    line-height: 22px;
    letter-spacing: 0em;
    text-align: left;
    color: var(--custom-font-color-primary);
  }
  .custom-accordion__content.accordion__content p{
    font-family: var(--custom-font-style-secondary);
    font-size: 16px;
    font-weight: 400;
    line-height: 19px;
    letter-spacing: 0em;
    text-align: left;
    color: var(--custom-font-color-primary);
    margin-bottom:15px;
  }
  .custom_reviews_text-main{
    display:flex;
    align-items:center;
    gap:10px;
  }
  .custom_reviews_text-main span{
    display:flex;
    align-items:center;
    gap:4px;
  }
  .custom_reviews_text-main span svg{
   min-width:18px;
   min-height:18px;
  }
  .custom_reviews_text-main p{
    font-family: var(--custom-font-style-secondary);
    font-size: 16px;
    font-weight: 300;
    line-height: 22px;
    letter-spacing: 0em;
    text-align: left;
    margin:0;
  }
  .custom_reviews_text-main p strong{
    font-weight: 700;
    padding:0 4px;
  }
  .custom-arrow_accordion{
    min-width:17px;
    min-height:17px;
  }
  /* Media Query */
  
  .custom-images_under_slider.mobile{
    display:none;
  }
  .custom-images_under_slider{
    display:flex;
    justify-content:space-between;
     margin-top:60px;
  }
  .custom-images_under_slider .custom-image{ 
    display:flex;
    flex-direction:column;
    align-items:center;
    text-align:center;
    max-width:176px;

  }
  .custom-images_under_slider .custom-image p{ 
    font-family: var(--custom-font-style-secondary);    
    font-size: 16px;
    font-weight: 400;
    line-height: 22px;
    letter-spacing: 0px;
    text-align: center;


  }

  .sticky_button_div {
    width: 100%;
    max-width: 450px;
  }
  .sticky_button_div .price{
    display:none;
}
  .custom-images_under_slider .custom-image img{ 
   max-width:60px;
   max-height:60px;

  }
  .custom-price{
    margin-top:20px;

  }
  .main-product-custom_product_info_wrapper .alireviews-review-star-rating{
display:none !important;
  }
  @media screen and (max-width: 1050px) {
    .main-product-custom_slider_wrapper .swiper-slide .swiper-small-img {
    height:60px;
    width:60px;
  }
    .custom-price{
      justify-content:center;
    }

   
    .custom_reviews_text-main p strong{
    padding:0 2px;
  }
    .custom_reviews_text-main{
    gap:4px;
    justify-content:center;
  }
    .custom_reviews_text-main span{
    gap:0;
  }
  .custom_reviews_text-main span svg{
   min-width:12px;
   min-height:12px;
   max-width:12px;
   max-height:12px;
  }
    .product__title h1 {
      font-size: 26px;
      line-height: 29px;
    }
   

    .custom-accordion__content p{
    font-size: 12px;
    line-height: 14px;
  }
  .custom_reviews_text-main p{
    font-size: 11px;
    line-height: 16px;
  }
 

  }
  @media screen and (max-width: 870px) {
    .sticky_button_custom_tittle-price h3{
    font-size:14px;
   }
   .sticky_button_custom_wrapper{
      padding:5px 10px;
   }
  }
  @media screen and (max-width: 750px) {

    .sticky_button_custom_tittle-price .custom_bage{
      display:none;

    }
  
    .custom-images_under_slider.mobile{
      display:flex;
    }
    .custom-images_under_slider.desc{
      display:none;
    }
    .custom-images_under_slider img{
        width:35px;
        height:35px;
        min-width:35px;
       min-height:35px;
    }
    .custom-images_under_slider {
      margin-top:20px;
    }
    .custom-images_under_slider p{
      font-size: 13px !important;
      line-height: 17px !important;
      text-align:left;
    }
    .custom-images_under_slider .custom-image{
     max-width:100px;
    }
    fieldset.custom-variants-block{ 
      display:flex;
      justify-content:center;
    }
    .product__title h1 {
      text-align: center;
    }
    .product_section_description{
      max-width:360px;
      margin:auto;    
    }
    .product_section_description li p {
      font-size: 14px;
      line-height: 16px;
    }
    .custom_product_under_tittle-main {
      font-size: 14px;
      line-height: 20px;
      margin:20px auto;
    }
    .product__accordion.accordion.quick-add-hidden details summary {
      min-height: 49px;
    }
    .custom-accordion__title{
    font-size: 16px;
    line-height: 20px;
  }

    quantity-input.quantity {
      width: 84px;
    }
    .quantity__button,
    .quantity__input {
      max-width: 28px;
    }
    .main-product-custom_wrapper {
      margin-top: 36px;
    }
    .main-product-custom_wrapper {
      align-items: center;
      flex-direction: column;
    }
    .qty_and_buy-btn_wrapper {
      gap: 9px;
    }
  }
  @media screen and (max-width: 640px) {
      .main-product-custom_slider_wrapper .swiper-slide .swiper-small-img {
    height:50px;
    width:50px;
  }
    .main-product-custom_slider_wrapper {
      padding: 0;
    }

    .main-product-custom_wrapper {
      margin-top: 36px;
    }

    .mySwiper_product2 .swiper-slide {
      padding: 0;
    }

    .swiper-main-img_wrapper,
    .main-product-custom_slider_wrapper .mySwiper_product2{
      max-height: 490px;
    }
 
  }

 
  @media screen and (max-width: 500px) {
    .swiper-main-img_wrapper,
    .main-product-custom_slider_wrapper .mySwiper_product2{
      max-height: 365px;
    }
    .main-product-custom_slider_wrapper {
      padding: 0;
    }
    .sticky_button_custom_tittle-price h3{
   display:none;
   }
   .sticky_button_custom_tittle-price .price {
    display:none;
   }
   .sticky_button_div .price{
    display:flex;
    margin-bottom: 5px;
    }
  }
  @media screen and (max-width: 400px) {
    .sticky_button_div .custom_bage{
      display:none;
    }
  }
  
  /* poduct info logic  */

  .main-product-custom_product_info_wrapper {
    max-width: 609px !important;
  }

  @media screen and (max-width: 749px) {
    .main-product-custom_product_info_wrapper {
      text-align: center;
    }
  }
    
  {%- endstyle -%}

  <script src="{{ 'product-info.js' | asset_url }}" defer="defer"></script>
  <script src="{{ 'product-form.js' | asset_url }}" defer="defer"></script>

  <div class="main-product-custom_wrapper grid grid--1-col {% if product.media.size > 0 %}grid--2-col-tablet{% else %}product--no-media{% endif %}">
  <div class="grid__item main-product-custom_slider_wrapper">
     
  <div class="swiper mySwiper_product2">
    <div class="swiper-wrapper">
      {% for link in product.images %}
        <div class="swiper-slide">
          <div class="swiper-main-img_wrapper">
            <img class="swiper-main-img" src="{{ link | image_url }}" alt="product_image"
              {%- if forloop.index == 1 -%}  
              loading="eager" fetchpriority="high"{%- else -%}
              loading="lazy"{%- endif -%} />
          </div>
        </div>
      {% endfor %}
    </div>
  
  </div>

