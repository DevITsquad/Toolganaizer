{{ 'component-cart.css' | asset_url | stylesheet_tag }}
{{ 'component-cart-items.css' | asset_url | stylesheet_tag }}
{{ 'component-totals.css' | asset_url | stylesheet_tag }}
{{ 'component-price.css' | asset_url | stylesheet_tag }}
{{ 'component-discounts.css' | asset_url | stylesheet_tag }}
{{ 'component-loading-overlay.css' | asset_url | stylesheet_tag }}

{%- style -%}
      .section-{{ section.id }}-padding {
        padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
        padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
      }

      @media screen and (min-width: 750px) {
        .section-{{ section.id }}-padding {
          padding-top: {{ section.settings.padding_top }}px;
          padding-bottom: {{ section.settings.padding_bottom }}px;
        }
      }

        .cart-page-wrapper_main{
          background: rgba(255, 253, 241, 1);
        }
      .cart-page-wrapper{
        max-width:1268px;
        margin:auto;
        padding:0 11px 64px 11px;
        box-sizing:border-box;
       

      }
      tr.cart-item.cart-item-custom{
        border: 1px solid rgba(216, 212, 212, 1) !important;
        border-radius:10px;
        display:flex;
        padding:29px 33px;
        max-width:800px;
        margin-bottom:20px;
        align-items:center;
        background:white;
        justify-content:space-between;
        max-height: 210px;
      }
      .custom_bage {
        display:flex;
        justify-content:center;
        align-items:center;
        height:28px;
        border-radius: 95px;
        background: rgba(251, 188, 4, 1);
        font-family: var(--custom-font-style-primary);
        font-size: 14px;
        font-weight: 600;
        line-height: 17px;
        letter-spacing: 0em;
        text-align: left;
        color:black;
        white-space:nowrap;
        padding: 0 10px;
        max-width:78%;
        margin:auto;
        margin-right:0;
      }
      .cart-item-custom img{
        border:none;
      }
      td.cart_img_wrapper{
        padding:0px;
      }
      .cart-item-custom td{
        padding:0;
      }
      .caption-with-letter-spacing,.underlined-link{
        display:none !important;
      }
      .cart-item__link-custom{
        display:flex;
        justify-content:center;
        align-items:center;
      }
      .cart-page_content_custom{
        display:flex;
        gap:26px;
      }

    #main-cart-footer{
      max-width:420px;
      width:100%;
      padding:30px 33px;
      background:white;
      border: 1px solid rgba(226, 233, 238, 1);
      margin:auto;
      border-radius:10px;
      margin-top:0;
    }
   

   
    .title-wrapper-with-link,
    .title-wrapper-with-link .markered_text,
    .title-wrapper-with-link .markered_text p{
    margin:0;
    }

    .title-wrapper-with-link{
      justify-content:center;
      flex-direction:column;
      align-items:center;
      padding-right:11px;
      padding-left:11px;
      padding-bottom:51px;
    }
    .title-wrapper-with-link{
      padding-top:97px;
      background: linear-gradient(180deg, #EFE9CC 0%, #FFFDF1 100%),
      linear-gradient(0deg, #FFFFFF, #FFFFFF);

    }
    .cart-item__link-custom,
    .cart-item__image-container{
      width:160px;
      height:160px;
    }
    .cart-item__image_custom {
      width:100%;
      height:100%;

    }
    .cart__blocks{
      display:flex;
      flex-direction:column;
      margin:0 !important;
      width:100%;
    }
    .cart__blocks > div{
     width:100%;
    
    }
    .cart__blocks > div small{
     text-align:center;
     margin:25px auto 10px auto;
  
    }
    .cart__blocks .totals{
      justify-content:space-between;
    }
    .cart__blocks .cart__ctas{
      margin:0;
    }
    .cart__footer{
      padding:0;

    }

    .dummy_bread_crumbs{
      display:flex;
      justify-content:space-between;
      max-width: 734px;
      width: 100%;
      margin:auto;

    }
    .dummy_bread_crumbs div{
      display:flex;
      align-items:center;
      gap:12px;
    }
    .dummy_bread_crumbs div svg{
      width:34px;
      height:34px;
    }
    .cart-item__totals.cart-item__totals_custom{
      display:flex;
      justify-content:space-between;
      flex-direction:column;
      align-items:flex-end;
      gap:54px;
    }
    .cart-item__totals_custom cart-remove-button a{
      text-decoration:underline !important;
      font-family:var(--custom-font-style-secondary);
      font-size: 16px;
      font-weight: 400;
      line-height: 16px;
      letter-spacing: 0em;
      text-align: left;
      color:var(--custom-font-color-primary);

    }
    .cart-item__details__custom{
      display:flex;
      flex-direction:column;
      height:100%;
      gap:20px;
    }
    .cart-quantity_custom{
      width: 114px;
    }
    .custom-drawer-price{
      display:flex;
      gap:5px;
      margin-bottom:12px;
    }
    .is-empty .cart__warnings{
    padding-top:0;
  }
    @media screen and (max-width:1300px){
      .cart-page_content_custom{
        flex-direction:column;
        gap:5px;
      }
      .cart-item-custom{
        margin:auto;

      }
      tr.cart-item.cart-item-custom{
        padding:17px;
       
      }
    }
    @media screen and (max-width:1150px){
      
      tr.cart-item.cart-item-custom{
        max-width:730px;
      }

     
      
    }
    @media screen and (max-width:767px){
      .cart-quantity_custom{
      width: 92px;
    }

      .cart-page-wrapper{
        padding:0 11px 50px 11px;
      }

      .cart-item__totals_custom cart-remove-button a{
        font-size: 12px;
        line-height: 12px;
      }
      .dummy_bread_crumbs div{
      gap:5px;
    }
      .dummy_bread_crumbs div span{
      font-size: 10px;
      font-weight: 600;
      line-height: 14px;
      letter-spacing: 0px;
      text-align: center;

    }
   

      .title-wrapper-with-link{
      padding-top:40px;
     padding-bottom:25px;
    }
      .cart-page_content_custom{
        flex-direction:column;
      }
      #main-cart-footer{

      padding:28px 17px;
    }

    .cart-item__link-custom,
    .cart-item__image-container{
      width:92px;
      height:92px;
    }
    .dummy_bread_crumbs div svg{
   width:18px;
   height:18px;
    }


    }

    @media screen and (max-width:480px){
      .title-wrapper-with-link{
        gap:26px;

      }
      .custom-drawer-price{
        flex-direction:column;
        gap:0;

      }

      .cart-item-custom{
        gap:0;
      }
      .cart-item__totals.cart-item__totals_custom{
      display:flex;
      justify-content:space-between;
      flex-direction:column;
      align-items:flex-end;
      gap:12px;
      align-self:flex-start;
    }
    tr.cart-item.cart-item-custom{
      padding:11px;
      max-height: 155px;
      margin-bottom:10px;
    }
    .cart-item__details__custom{
      gap:0;

    }

    .custom_bage{
      max-width:100%;
        margin:auto;
    }
    
    }
   

  
{%- endstyle -%}

{%- unless settings.cart_type == 'drawer' -%}
  <script src="{{ 'cart.js' | asset_url }}" defer="defer"></script>
{%- endunless -%}
